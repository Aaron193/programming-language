function makeAdder(n) {
  function addOne(x) {
    return n + x;
  }
  return addOne;
}

var funcs = [];

var start = clock();

for (var i = 0; i < 10000; i++) {
  funcs.push(makeAdder(i));
}

var sum = 0;
for (var i = 0; i < funcs.size(); i++) {
  sum += funcs[i](1);
}

var elapsed = clock() - start;

print sum;
print elapsed;
