auto n = 5000;
auto arr = [];

for (auto value = n; value > 0; value--) {
  arr.push(value);
}

auto start = clock();

auto i = 1;
while (i < arr.size()) {
  auto key = arr[i];
  auto j = i - 1;
  auto shifting = true;

  while (j >= 0 and shifting) {
    if (arr[j] > key) {
      arr[j + 1] = arr[j];
      j = j - 1;
    } else {
      shifting = false;
    }
  }

  arr[j + 1] = key;
  i = i + 1;
}

auto elapsed = clock() - start;

print arr[0];
print arr[n - 1];
print elapsed;