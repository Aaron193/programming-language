var n = 5000;
var arr = [];

for (var value = n; value > 0; value--) {
  arr.push(value);
}

var start = clock();

var i = 1;
while (i < arr.size()) {
  var key = arr[i];
  var j = i - 1;
  var shifting = true;

  while (j >= 0 and shifting) {
    if (arr[j] > key) {
      arr[j + 1] = arr[j];
      j = j - 1;
    } else {
      shifting = false;
    }
  }

  arr[j + 1] = key;
  i = i + 1;
}

var elapsed = clock() - start;

print arr[0];
print arr[n - 1];
print elapsed;