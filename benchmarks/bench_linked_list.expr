class Node {}

var start = clock();

var head = Node();
head.value = 1;
head.next = head;

var tail = head;
for (var i = 2; i <= 100000; i++) {
  var node = Node();
  node.value = i;
  node.next = head;
  tail.next = node;
  tail = node;
}

var sum = 0;
var current = head;
for (var steps = 0; steps < 100000; steps++) {
  sum += current.value;
  current = current.next;
}

var elapsed = clock() - start;

print sum;
print elapsed;
