class Node {}

auto start = clock();

auto head = Node();
head.value = 1;
head.next = head;

auto tail = head;
for (auto i = 2; i <= 100000; i++) {
  auto node = Node();
  node.value = i;
  node.next = head;
  tail.next = node;
  tail = node;
}

auto sum = 0;
auto current = head;
for (auto steps = 0; steps < 100000; steps++) {
  sum += current.value;
  current = current.next;
}

auto elapsed = clock() - start;

print sum;
print elapsed;
