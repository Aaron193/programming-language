var limit = 100000;
var composite = [];

for (var i = 0; i <= limit; i++) {
  composite.push(false);
}

var start = clock();

i64 p = 2;
while (p * p <= limit) {
  if (!composite[p]) {
    i64 multiple = p * p;
    while (multiple <= limit) {
      composite[multiple] = true;
      multiple = multiple + p;
    }
  }
  p = p + 1;
}

var count = 0;
i64 n = 2;
while (n <= limit) {
  if (!composite[n]) count += 1;
  n = n + 1;
}

var elapsed = clock() - start;

print count;
print elapsed;
